{
  "colors": {
    "primary": "#FCBB75",
    "secondaryBackground": "#FFE9D6",
    "panelBackground": "#FBD6B5",
    "cardBackground": "#FFF3EB",
    "accentRed": "#E64A19",
    "accentGreen": "#388E3C",
    "textDark": "#4A3C2A",
    "textLight": "#8E8E8E",
    "white": "#FFFFFF"
  },
  "typography": {
    "fontFamily": ["Inter", "Nunito Sans", "Roboto", "sans-serif"],
    "sizes": {
      "title": "22px",
      "header": "18px",
      "body": "14px",
      "small": "12px"
    },
    "weights": {
      "regular": 400,
      "medium": 500,
      "semiBold": 600,
      "bold": 700
    }
